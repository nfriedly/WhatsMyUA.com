   <link rel="stylesheet" href="/stylesheets/wmua.css" type="text/css" media="screen, projection">

</head>
<body>
<div class="container main">

<%= yield %>

<div class="clearfix"></div>
</div>

<div class="footer container">
<p>
	<a href="/">WhatsMyUA.com</a> 
	by <a href="http://nfriedly.com/webdev" title="Cincinnati Ruby On Rails">nFriedly Web Development</a>
