   <link rel="stylesheet" href="/stylesheets/bluetrip/css/screen.css" type="text/css" media="screen, projection">
   <link rel="stylesheet" href="/stylesheets/bluetrip/css/print.css" type="text/css" media="print"> 
   <!--[if IE]>
